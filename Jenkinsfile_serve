pipeline {
    agent {
        docker {
            label 'docker_node2'
            image 'redbeard28/docset:0.1'
            args '-p 8001:8000'
        }
    }
    options {
        timeout(6, HOURS)
    }
    stages {
        stage(‘Build’) {
            steps {
                sh ‘mvn clean install’
            }
        }
    }
    post {
        always {
            deleteDir()
        }
    }
}